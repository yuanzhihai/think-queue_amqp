{
  "name": "yzh52521/think-queue-amqp",
  "description": "think-queue for ThinkPHP6 amqp",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "think\\": "src"
    },
    "files": [
      "src/common.php"
    ]
  },
  "minimum-stability": "dev",
  "require": {
    "ext-json": "*",
    "topthink/think-helper": "^3.0.4",
    "topthink/framework": "^6.0.0",
    "topthink/think-factory": "^1.0.0",
    "symfony/process": "^4.2",
    "nesbot/carbon": "^2.16",
    "php-amqplib/php-amqplib": "^2.12.1"
  },
  "extra": {
    "think": {
      "services": [
        "think\\queue\\Service"
      ],
      "config": {
        "queue": "src/config.php"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^1.2",
    "topthink/think-migration": "^3.0.0"
  },
  "conflict": {
    "topthink/think-queue": "*"
  }

}
