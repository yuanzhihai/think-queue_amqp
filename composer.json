{
  "name": "yzh52521/think-queue-amqp",
  "description": "think-queue for ThinkPHP6 amqp",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "think\\": "src"
    },
    "files": [
      "src/common.php"
    ]
  },
  "minimum-stability": "dev",
  "require": {
    "ext-json": "*",
    "php-amqplib/php-amqplib": "^2.12.1"
  },
  "extra": {
    "think": {
      "services": [
        "think\\queue\\Service"
      ],
      "config": {
        "queue": "src/config.php"
      }
    }
  }

}
